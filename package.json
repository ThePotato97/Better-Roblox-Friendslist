{
  "name": "better-roblox-friendslist",
  "version": "0.0.1",
  "description": "A better friendslist for roblox",
  "author": "ThePotato97/Better-Roblox-Friendslist",
  "license": "ISC",
  "repository": "",
  "private": true,
  "scripts": {
    "prepare": "npx husky install",
    "start": "parcel watch src/manifest.json --host localhost --config @parcel/config-webextension",
    "build": "parcel build src/manifest.json",
    "build:analyzer": "npx cross-env ANALYZER=true npx node utils/build.js",
    "type-check": "npx tsc --noEmit",
    "lint": "npm run eslint && npm run stylelint",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "npx eslint src/**/*.{js,jsx,ts,tsx}",
    "eslint:fix": "npx eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "stylelint": "npx stylelint src/**/*.{css,less}",
    "stylelint:fix": "npx stylelint --fix src/**/*.{css,less}",
    "prettier": "npx prettier --check src/**/*.{js,jsx,ts,tsx}",
    "prettier:fix": "npx prettier --write src/**/*.{js,jsx,ts,tsx}",
    "webext:pack": "web-ext build --source-dir build"
  },
  "devDependencies": {
    "@parcel/compressor-brotli": "^2.8.3",
    "@parcel/compressor-gzip": "^2.8.3",
    "@parcel/config-webextension": "^2.8.3",
    "@parcel/reporter-bundle-analyzer": "^2.8.3",
    "@parcel/transformer-inline-string": "^2.8.3",
    "@parcel/transformer-sass": "2.8.3",
    "@types/chrome": "^0.0.234",
    "@types/parcel-env": "^0.0.1",
    "@types/react": "^18.0.28",
    "@types/serviceworker": "^0.0.62",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.39.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-plugin": "^5.0.8",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "openapi-zod-client": "^1.6.3",
    "parcel": "^2.8.3",
    "preact-jsx-runtime": "^1.2.0",
    "prettier": "^2.8.4",
    "process": "^0.11.10",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.1",
    "@mui/material": "^5.12.2",
    "@parcel/reporter-bundle-buddy": "^2.8.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@szhsin/react-menu": "^3.5.2",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/parser": "^5.53.0",
    "@zodios/core": "^10.9.0",
    "bottleneck": "^2.19.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-popper": "^2.3.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "rozod": "^2.0.2",
    "typeface-roboto": "^1.1.13",
    "webext-storage-cache": "^5.1.1",
    "zod": "^3.21.4"
  }
}
